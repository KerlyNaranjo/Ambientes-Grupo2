{% extends 'base.html.twig' %}

{% block body %}
    {{ parent() }}
    <div class="container">
        <div class="cold-md-12">
            <div class="page-header margin-none">
                <h1>Cooperativa</h1>
            </div>
            <div class="table-responsive">
                <table class="table table-striped table-hover">
                    <tbody>
                    <tr>
                        <th>Id</th>
                        <td>{{ cooperativa.id }}</td>
                    </tr>
                    <tr>
                        <th>Nombrecoop</th>
                        <td>{{ cooperativa.nombreCoop }}</td>
                    </tr>
                    <tr>
                        <th>Ruccoop</th>
                        <td>{{ cooperativa.rucCoop }}</td>
                    </tr>
                    </tbody>
                </table>
                <ul>
                    <li>
                        <a href="{{ path('cooperativa_index') }}" class="btn btn-sm btn-info">Back to the list</a>
                        <a href="{{ path('cooperativa_edit', { 'id': cooperativa.id }) }}" class="btn btn-sm btn-primary">Edit</a>
                        {{ form_start(delete_form) }}
                        <input type="submit" value="Delete" class="btn btn-sm btn-danger btn-delete">
                        {{ form_end(delete_form) }}
                    </li>
                </ul>
            </div>

        </div>

    </div>



{% endblock %}

