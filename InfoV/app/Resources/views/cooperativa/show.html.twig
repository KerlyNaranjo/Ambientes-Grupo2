            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cooperativa_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cooperativa_edit', { 'id': cooperativa.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
