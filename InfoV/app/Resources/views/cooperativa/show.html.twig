{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cooperativa</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cooperativa.id }}</td>
            </tr>
            <tr>
                <th>Nombrecooperativa</th>
                <td>{{ cooperativa.nombreCooperativa }}</td>
            </tr>
            <tr>
