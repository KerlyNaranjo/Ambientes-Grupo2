{% extends 'menu.html.twig' %}

{% block title %}Infoviajes{% endblock %}

{% block body %}
    {{ parent() }}
<div class="container">
    <div class="cold-md-12">
        <div class="page-header margin-none">
            <h1>Registro exitoso</h1>
        </div>
        <div class="table-responsive">
            <table class="table table-striped table-hover">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ destino.id }}</td>
            </tr>
            <tr>
                <th>Lugardestino</th>
                <td>{{ destino.lugarDestino }}</td>
            </tr>
            <tr>
                <th>Tiempodestino</th>
                <td>{{ destino.tiempoDestino }}</td>
            </tr>
            <tr>
                <th>Preciodestino</th>
                <td>{{ destino.precioDestino }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('destino_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('destino_edit', { 'id': destino.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
